{
  "name": "create-web-components-v1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm-run-all lint:*",
    "lint:js": "npx eslint \"./**/*.{js,jsx,ts,tsx}\" --no-error-on-unmatched-pattern",
    "lint:css": "npx stylelint \"./**/*.{scss,css}\" --allow-empty-input",
    "lint:prettier": "npx prettier \"./**/*.{js,jsx,ts,tsx}\" --list-different --no-error-on-unmatched-pattern",
    "fix": "npm-run-all fix:*",
    "fix:css": "npm run lint:css -- --fix",
    "fix:js": "npm run lint:js -- --fix",
    "fix:prettier": "prettier \"./**/*.{js,jsx,ts,tsx,css}\" --write --no-error-on-unmatched-pattern"
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@monaco-editor/react": "^4.6.0",
    "next": "13.5.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "eslint": "^8",
    "eslint-config-next": "13.5.4",
    "postcss": "^8",
    "prettier": "3.0.3",
    "tailwindcss": "^3",
    "typescript": "^5"
  }
}
